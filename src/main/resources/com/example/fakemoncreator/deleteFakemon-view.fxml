<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.fakemoncreator.deleteFakemonController"
      style="-fx-background-color:#ffffff;">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label layoutX="95" layoutY="40" text="Select the fakemon you would like to delete, then"/>
    <Label layoutX="135" layoutY="55" text="click the Delete Fakemon button."/>

    <ComboBox fx:id="fakemonBox" layoutX="90" layoutY="110" prefWidth="260"/>

    <Button onAction="#deleteFakemon" layoutX="167" layoutY="180" text="Delete Fakemon"/>
</Pane>
